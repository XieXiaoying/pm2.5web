.force{
    font-size: 16px;
}
.content{
    padding: 0 40px;
}
.text-center{
    display: flex;
    padding: 0 200px;
    justify-content: space-between;   
}
.imgBox {
    flex-direction: row;
    flex-wrap: nowrap;
    margin: 0 auto;
    float: left;
    flex-wrap: wrap;
    justify-content: space-around;
    align-content: space-between;
}
.box_img{
    width: 200px;
    flex-shrink: 0;
    padding: 5px;
    border: 1px solid #cccccc;
    box-shadow:0 0 5px #ccc;
    border-radius: 5px;
}
.box_img img{
    width: 100%;
    height: 100%;
}
.glyphicon-trash{
    float: right;
}
